FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pIon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 4e-12;

boundaryField
{
    erf1
    {
        type            fixedValue;
        value           uniform 0;
    }
    erf2
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
    es1
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
    es2
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}


